moda <- function(x){
mayor <- max(x)
numeros <- 1:mayor
conteo <- tabulate(x)
validos <- conteo == max(conteo)
resultado <- (numeros*validos)
return(resultado)
}
lista <- list(c(1,2,2,3),c(1,1,1,2,3),c(3,3,3,2,2,1))
lapply(lista,moda)
