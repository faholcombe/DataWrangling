juego <- function(){
inicio <- 1
while(inicio == 1){
numero <- round(runif(1,1,10))
print("¿Adivina que número estoy pensando? Ingresa un numero entero del 1 al 10 a la consola.")
guess <- scan("", double(), n = 1)
while(guess%%1 != 0 | guess<0 | guess>10){
print("Eso no es una opción valida.")
guess <- scan("", double(), n = 1)
}
while (guess != numero) {
print("Numéro incorrecto, ingrese un numero entero del 1 al 10: ")
guess <- scan("", double(), n = 1)
}
print(paste("Adivinaste mi numero! Mi numero era:",guess))
pregunta <- 1
while(pregunta == 1){
print("¿Quieres jugar de nuevo? [Y/N]")
seguir <- scan("", "character", n = 1)
if(seguir == "Y"){
inicio <- 1
pregunta <- 0
} else {
if (seguir =="N"){
inicio <- 0
pregunta <- 0
} else {
print("No se reconocio tu respuesta.")
pregunta <- 1
}
}
}
}
}
juego()
